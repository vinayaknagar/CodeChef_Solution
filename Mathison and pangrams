# cook your dish here Vinayak Nagar
for _ in range(int(input())):
    number=list(map(int,input().split()))
    string=input()
    
    alpha="abcdefghijklmnopqrstuvwxyz"
    
    add=0
    for i in range(26):
        if alpha[i] not in string:
            add+=number[alpha.index(alpha[i])]
    print(add)
    
    
